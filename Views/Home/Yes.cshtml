        if (posY < 0 || posY > window.innerHeight) dirY *= -1;
        
        minion.style.left = posX + 'px';
        minion.style.top = posY + 'px';
        minion.style.transform = `rotate(${Math.random() * 20 - 10}deg) scale(${0.8 + Math.random() * 0.4})`;
        
        requestAnimationFrame(animate);
      };
      
      animate();
      
      setTimeout(() => minion.remove(), 8000);
    }

